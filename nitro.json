{
  "$schema": "https://nitro.margelo.com/nitro.schema.json",
  "cxxNamespace": ["vasana"],
  "ios": {
    "iosModuleName": "NitroVasana"
  },
  "android": {
    "androidNamespace": ["vasana"],
    "androidCxxLibName": "NitroVasana"
  },
  "autolinking": {
    "Math": {
